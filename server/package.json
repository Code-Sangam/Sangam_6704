{
  "name": "sangam-alumni-network-server",
  "version": "1.0.0",
  "description": "Traditional Express.js server for Sangam Alumni Network",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:simple": "node app-simple.js",
    "start:full": "node app.js",
    "start:railway": "NODE_ENV=production node app-simple.js",
    "dev": "nodemon app.js",
    "build": "echo 'No build step needed'",
    "build:webpack": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "node scripts/test-integration.js",
    "verify": "node scripts/verify-setup.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "sequelize-cli": "^6.6.2",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [
    "express",
    "alumni",
    "network",
    "chat",
    "traditional-web"
  ],
  "author": "Sangam Alumni Network Team",
  "license": "MIT"
}
